{
  "description": "test",
  "processors": [
    {
      "sycamore_ingest": {
        "input_field": "data",
        "output_field": "extracted",
        "aryn_api_key": "__ARYN_API_KEY__",
        "ignore_missing": true,
        "threshold": "auto",
        "use_ocr": true,
        "extract_images": true,
        "extract_table_structure": true,
        "summarize_images": true,
        "ocr_language": "english",
        "chunking_options": {
          "strategy": "maximize_within_limit",
          "tokenizer": "openai_tokenizer",
          "tokenizer_options": {
            "model_name": "text-embedding-3-small"
          },
          "merge_across_pages": true,
          "max_tokens": 512
        }
      }
    }
  ]
}